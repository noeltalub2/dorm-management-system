<script>
    const Toast = Swal.mixin({
        toast: true,
        position: "top-end",
        showConfirmButton: false,
        timer: 4000,
        timerProgressBar: true,
    })

    if ('<%-success_msg%>' != '') {

        Toast.fire({
            icon: 'success',
            title: "<%=success_msg%>"
        })
    }

    if ('<%-error_msg%>' != '') {

        Toast.fire({
            icon: 'error',
            title: "<%=error_msg%>"
        })
    }
</script>